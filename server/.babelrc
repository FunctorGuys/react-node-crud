{
  "presets": [
    "babel-preset-env"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@/config": "./src/config",
        "@/api": "./src/api",
        "@/views": "./src/views",
        "@/models": "./src/models",
        "@/services": "./src/services",
        "@/loaders": "./src/loaders",
      }
    }]
  ]
}